local function filter_events()
 local n=0
 for i,v in pairs(SetRemotes:GetChildren()) do
  v.Visible=not (FilterE.TextColor3==Color3.fromRGB(0,120,215) and v.Icon.Image=="rbxassetid://413369623")
  if v.Visible then n=n+1 v.Position=UDim2.new(0,10,0,-20+n*30) else v.Position=UDim2.new(0,10,0,-20+i*30) end
 end
end
local function filter_functions()
 local n=0
 for i,v in pairs(SetRemotes:GetChildren()) do
  v.Visible=not (FilterF.TextColor3==Color3.fromRGB(0,120,215) and v.Icon.Image=="rbxassetid://413369506")
  if v.Visible then n=n+1 v.Position=UDim2.new(0,10,0,-20+n*30) else v.Position=UDim2.new(0,10,0,-20+i*30) end
 end
end
local function onclick_fevents() FilterE.TextColor3=FilterE.TextColor3==Color3.fromRGB(0,120,215) and Color3.fromRGB(150,150,150) or Color3.fromRGB(0,120,215) FilterE.BorderColor3=FilterE.TextColor3==Color3.fromRGB(150,150,150) and Color3.fromRGB(80,80,80) or Color3.fromRGB(0,90,160) filter_events() end
