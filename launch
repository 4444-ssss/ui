local _refs={BG=BG,Ribbon=Ribbon,Title=Title,Hide=Hide,FullScreen=FullScreen,ButtonsFrame=ButtonsFrame,Remotes=Remotes,Source=Source,SetRemotes=SetRemotes,SetRemotesTab=SetRemotesTab,ToClipboard=ToClipboard,Decompile=Decompile,GetReturn=GetReturn,ClearList=ClearList,CryptStrings=CryptStrings,EnableSpy=EnableSpy,Settings=Settings,Last=Last,Total=Total,Search=Search,FilterE=FilterE,FilterF=FilterF}
local function _safe(c) return typeof(c)=="Instance" and c or nil end
local function _paintButton(b,theme,kind) if not _safe(b) then return end b.BackgroundColor3=theme.bg2 b.BorderColor3=theme.border b.TextColor3=(kind=="accent") and theme.accent or theme.text end
local function _paintScroll(s,theme) if not _safe(s) then return end s.BackgroundColor3=theme.bg2 s.BorderColor3=theme.border s.ScrollBarImageColor3=theme.muted end
local function _paintRemoteRow(row,theme)
 if not _safe(row) or not row:IsA("TextButton") then return end
 row.BackgroundColor3=theme.bg2
 row.BorderColor3=theme.border
 local rn=row:FindFirstChild("RemoteName")
 local id=row:FindFirstChild("ID")
 local ic=row:FindFirstChild("Icon")
 local en=row:FindFirstChild("Enabled")
 if _safe(rn) then rn.BackgroundColor3=theme.accent rn.TextColor3=theme.text end
 if _safe(id) then if id:IsA("TextLabel") then id.BackgroundColor3=theme.bg2 id.BorderSizePixel=id.BorderSizePixel or 1 id.BorderColor3=theme.border id.TextColor3=theme.muted end end
 if _safe(ic) then ic.ImageColor3=Color3.fromRGB(255,255,255) end
 if _safe(en) then en.BackgroundColor3=theme.bg2 en.BorderColor3=theme.accent en.TextColor3=theme.accent end
end
local function _recolorAll(theme)
 if _safe(_refs.BG) then _refs.BG.BackgroundColor3=theme.bg _refs.BG.BorderColor3=theme.border end
 if _safe(_refs.Ribbon) then _refs.Ribbon.BackgroundColor3=theme.ribbon _refs.Ribbon.BorderSizePixel=0 end
 for _,k in ipairs({"Title","Hide","FullScreen"}) do local el=_refs[k] if _safe(el) then el.BackgroundColor3=theme.ribbon el.TextColor3=theme.text end end
 local f=_refs.ButtonsFrame if _safe(f) then f.BackgroundColor3=theme.bg2 f.BorderColor3=theme.border end
 _paintButton(_refs.ToClipboard,theme,"accent")
 _paintButton(_refs.Decompile,theme)
 _paintButton(_refs.GetReturn,theme)
 _paintButton(_refs.ClearList,theme)
 _paintButton(_refs.CryptStrings,theme,"accent")
 _paintButton(_refs.EnableSpy,theme,"accent")
 _paintButton(_refs.Settings,theme,"accent")
 _paintScroll(_refs.Remotes,theme)
 _paintScroll(_refs.Source,theme)
 _paintScroll(_refs.SetRemotes,theme)
 _paintScroll(_refs.SetRemotesTab,theme)
 for _,lab in ipairs({_refs.Last,_refs.Total}) do if _safe(lab) then lab.BackgroundColor3=theme.bg2 lab.BorderColor3=theme.border lab.TextColor3=theme.text end end
 if _safe(_refs.Search) then _refs.Search.BackgroundColor3=theme.bg2 _refs.Search.BorderColor3=theme.border _refs.Search.TextColor3=theme.muted end
 _paintButton(_refs.FilterE,theme,"accent")
 _paintButton(_refs.FilterF,theme,"accent")
 if _safe(Remotes) then for _,row in ipairs(Remotes:GetChildren()) do _paintRemoteRow(row,theme) end end
 if _safe(SetRemotes) then for _,row in ipairs(SetRemotes:GetChildren()) do _paintRemoteRow(row,theme) end end
end
local currentThemeIndex=1
local rainbowConn=nil
local function _stopRainbow() if rainbowConn then rainbowConn:Disconnect() rainbowConn=nil end end
local function applyThemeByName(name)
 local theme=THEMES[name] if not theme then return end
 if name~="Rainbow" then _stopRainbow() end
 _recolorAll(theme)
 if name=="Rainbow" then
  local t0=tick()
  _stopRainbow()
  rainbowConn=game:GetService("RunService").Heartbeat:Connect(function()
   local t=tick()-t0
   local h=(t*0.15)%1
   local accent=Color3.fromHSV(h,0.85,1)
   local ribbon=accent
   local rt={bg=theme.bg,bg2=theme.bg2,ribbon=ribbon,border=theme.border,text=theme.text,muted=theme.muted,accent=accent}
   _recolorAll(rt)
  end)
 end
 if _safe(ThemeBtn) then ThemeBtn.Text="THEME: "..name end
end
local function nextTheme() currentThemeIndex=currentThemeIndex%#THEME_ORDER+1 applyThemeByName(THEME_ORDER[currentThemeIndex]) end
if _safe(ThemeBtn) then ThemeBtn.Text="THEME: "..THEME_ORDER[currentThemeIndex] ThemeBtn.MouseButton1Down:Connect(nextTheme) end
applyThemeByName(THEME_ORDER[currentThemeIndex])
