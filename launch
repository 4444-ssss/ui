local function HighlightStr(s,keywords)
 local K={} local S=s
 local Token={["="]=true,["."]=true,[","]=true,["("]=true,[")"]=true,["["]=true,["]"]=true,["{"]=true,["}"]=true,[":"]=true,["*"]=true,["/"]=true,["+"]=true,["-"]=true,["%"]=true,[";"]=true,["~"]=true}
 for _,v in pairs(keywords) do K[v]=true end
 S=S:gsub(".",function(c) if Token[c]~=nil then return "\32" else return c end end)
 S=S:gsub("%S+",function(c) if K[c]~=nil then return c else return (" "):rep(#c) end end)
 return S
end
local function TokensOnly(s)
 local Token={["="]=true,["."]=true,[","]=true,["("]=true,[")"]=true,["["]=true,["]"]=true,["{"]=true,["}"]=true,[":"]=true,["*"]=true,["/"]=true,["+"]=true,["-"]=true,["%"]=true,[";"]=true,["~"]=true}
 local A=""
 s:gsub(".",function(c) if Token[c]~=nil then A=A..c elseif c=="\n" then A=A.."\n" elseif c=="\t" then A=A.."\t" else A=A.."\32" end end)
 return A
end
local function strings_mask(s)
 local highlight="" local quote=false
 s:gsub(".",function(c)
  if quote==false and c=="\"" then quote=true elseif quote==true and c=="\"" then quote=false end
  if quote==false and c=="\"" then highlight=highlight.."\"" elseif c=="\n" then highlight=highlight.."\n" elseif c=="\t" then highlight=highlight.."\t" elseif quote==true then highlight=highlight..c elseif quote==false then highlight=highlight.."\32" end
 end)
 return highlight
end
local function comments_mask(s)
 local ret=""
 s:gsub("[^\r\n]+",function(c)
  local comm=false local i=0
  c:gsub(".",function(n) i=i+1 if c:sub(i,i+1)=="--" then comm=true end if comm then ret=ret..n else ret=ret.."\32" end end)
  ret=ret
 end)
 return ret
end
local function copy_source()
 local scr=""
 for _,v in pairs(Source:GetChildren()) do scr=scr..v.SourceText.Text.."\n" end
 local copy
 if Clipboard~=nil then copy=Clipboard.set elseif Synapse~=nil then copy=function(str) Synapse:Copy(str) end elseif setclipboard~=nil then copy=setclipboard end
 if copy then copy(scr) end
end
local function onclick_fullscreen() size_pos_frame(BG,UDim2.new(1,0,1,40),UDim2.new(0,0,0,-40)) BG.Draggable=false end
local function filter_remotes(kind)
 local n=0
 if kind=="Text" then
  for i,v in pairs(SetRemotes:GetChildren()) do
   if v.Name:lower():match(Search.Text:lower()) and string~="" then v.Visible=true n=n+1 else v.Visible=false end
   if v.Visible then v.Position=UDim2.new(0,10,0,-20+n*30) else v.Position=UDim2.new(0,10,0,-20+i*30) end
  end
 end
end
